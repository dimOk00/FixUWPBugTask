<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Foundation.Controls"
    xmlns:local2="using:Foundation.Controls.Imaging"
    xmlns:converters="using:GhostCore.UWP.Converters"
    xmlns:controls="using:Microsoft.Toolkit.Uwp.UI.Controls">

    <Style TargetType="local2:UnrestrictedImage" >
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local2:UnrestrictedImage">
                    <Image x:Name="PART_Image" Stretch="{TemplateBinding Stretch}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>
